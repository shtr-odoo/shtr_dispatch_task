<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_transport_dispatch_setting" model="ir.ui.view">
            <field name="name">stock.transport.dispatch.view</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//block[@name='shipping_setting_container']" position='inside'>
                    <setting id='stock_transport_dispatch_sysytem'>
                        <field name="module_stock_transport" company_dependent="1" help="For installing the Dispatch Management System"/>
                    </setting>
                </xpath>
            </field>
        </record>
    </data>
</odoo>